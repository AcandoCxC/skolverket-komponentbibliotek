{{#each sections }}
<div class="skv-section skv-section--boxed">
  <header class="skv-section__header">
    <h2 class="h3 skv-section__header-title"><span class="skv-section__header-title-text">{{ title }}</span></h2>
    <div class="skv-section__header-preamble text-secondary">
      {{{ description }}}
    </div>
  </header>
  <div class="skv-section__body">
    {{{ body }}}
  </div>
</div>
{{/each}}
