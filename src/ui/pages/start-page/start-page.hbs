<div class="container--header">
  <div class="container">
    {{ render '@box-link-container' }}
    <div class="skv-section skv-section--small skv-section--last">
      {{ render '@link-list' }}
    </div>
  </div>
</div>
<div class="container container--no-padding">
  <div class="skv-box skv-box--no-padding">
    {{ render '@card-container' }}
    <div class="skv-section skv-section--boxed">
      <header class="skv-section__header">
        <h2 class="h3 skv-section__header-title"><span class="skv-section__header-title-text">Sektionstitel<span></h2>
        <div class="skv-section__header-preamble text-secondary">
          <p>Kontextuell fördjupningstext <a href="/press" class="text-link">med länk</a></p>
        </div>
      </header>
      <div class="skv-section__body">
        <img src="http://placehold.it/800x250/ffffff/E8D3E0?text=(Sektionens innehåll)" alt="Exempelbild" style="max-width: 100%" />
      </div>
    </div>
  </div>
</div>
