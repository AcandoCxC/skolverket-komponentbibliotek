<div class="container container--margins">
  <h2 class="h2 margin-tp-reset">{{pageTitle}}</h2>
  <p class="text-large">{{pageSubtitle}}</p>
  {{ render '@box-link-container' }}
</div>
<div class="container container--margins">
  {{ render '@link-list--outlined' linkList merge=true }}
</div>
<div class="container container--no-padding">
  <div class="skv-box">
    {{ render '@content-navigation' contentNav }}
    <div class="skv-section skv-section--last">
      {{ render '@card-container' }}
    </div>
  </div>
</div>
