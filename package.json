    "babelify": "~8.0.0",
    "browserify": "~14.5.0",
    "del": "~3.0.0",
    "gh-pages": "^1.1.0",
    "gulp": "~3.9.1",
    "gulp-autoprefixer": "~4.0.0",
    "gulp-environments": "~0.1.2",
    "gulp-flatten": "~0.3.1",
    "gulp-rename": "~1.2.2",
    "gulp-replace": "~0.6.1",
    "gulp-sass": "~3.1.0",
    "gulp-sequence": "~0.4.6",
    "gulp-sourcemaps": "~2.6.1",
